<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">prasanna_invitecode_invite_code_form.invite_code_form_data_source</item>
		</item>
		<item name="label" xsi:type="string" translate="true">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button name="back" class="Prasanna\Invitecode\Block\Adminhtml\Invite\Code\Edit\BackButton"/>
			<button name="delete" class="Prasanna\Invitecode\Block\Adminhtml\Invite\Code\Edit\DeleteButton"/>
			<button name="save" class="Prasanna\Invitecode\Block\Adminhtml\Invite\Code\Edit\SaveButton"/>
			<button name="save_and_continue" class="Prasanna\Invitecode\Block\Adminhtml\Invite\Code\Edit\SaveAndContinueButton"/>
		</buttons>
		<namespace>prasanna_invitecode_invite_code_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>prasanna_invitecode_invite_code_form.invite_code_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="invite_code_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider name="invite_code_form_data_source" class="Prasanna\Invitecode\Model\Invitecode\DataProvider">
			<settings>
				<requestFieldName>invite_code_id</requestFieldName>
				<primaryFieldName>invite_code_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field name="entity_id" formElement="input" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">invite_code</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">entity_id</label>
				<dataScope>entity_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="attribute_code" formElement="input" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">invite_code</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">attribute_code</label>
				<dataScope>attribute_code</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="code" formElement="input" sortOrder="30">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">invite_code</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">code</label>
				<dataScope>code</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="reusable" formElement="input" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">invite_code</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">reusable</label>
				<dataScope>reusable</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="case_sensitive" formElement="input" sortOrder="50">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">invite_code</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">case_sensitive</label>
				<dataScope>case_sensitive</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="customer_group" formElement="input" sortOrder="60">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">invite_code</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">customer_group</label>
				<dataScope>customer_group</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="count" formElement="input" sortOrder="70">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">invite_code</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">count</label>
				<dataScope>count</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="created_date" formElement="date" sortOrder="80">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">invite_code</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">created_date</label>
				<dataScope>created_date</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>
